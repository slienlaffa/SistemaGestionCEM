    <class>modelo.entity.Genero</class>
    <class>modelo.entity.Usuario</class>
    <class>modelo.entity.FamiliaAnfitriona</class>
    <class>modelo.entity.Antecedentes</class>
    <class>modelo.entity.Pais</class>
    <class>modelo.entity.Ciudad</class>
    <class>modelo.entity.Alumno</class>
    <class>modelo.entity.CentroEstudioLocal</class>
    <class>modelo.entity.TipoUsuario</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties/>
